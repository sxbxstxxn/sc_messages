{
    "name": "SebastianChristoph/sc_messages",
    "type": "typo3-cms-extension",
    "description": "Typo3 Extension for frontend users sind messages to other frontend users",
    "authors": [
        {
            "name": "Sebastian Christoph",
            "role": "Developer"
        }
    ],
    "version": "1.0.0",
    "require": {
        "typo3/cms-core": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "SebastianChristoph\\ScMessages\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SebastianChristoph\\ScMessages\\Tests\\": "Tests"
        }
    },
    "replace": {
        "sc_messages": "self.version",
        "typo3-ter/sc_messages": "self.version"
    }
}